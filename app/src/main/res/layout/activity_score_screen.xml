<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ScoreScreen">

    <ImageView
        android:id="@+id/complete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/complete11"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <ImageButton
        android:id="@+id/home"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/home_bt"
        app:layout_constraintBottom_toBottomOf="@id/complete"
        app:layout_constraintStart_toStartOf="@id/complete"
        app:layout_constraintEnd_toStartOf="@id/reset"
        android:layout_marginBottom="70dp"
        />

    <ImageButton
        android:id="@+id/reset"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/complete_reset"
        app:layout_constraintBottom_toBottomOf="@id/complete"
        app:layout_constraintStart_toEndOf="@id/home"
        app:layout_constraintEnd_toStartOf="@id/ans"
        android:layout_marginBottom="70dp"
        />

     <ImageButton
         android:id="@+id/ans"
         android:layout_width="100dp"
         android:layout_height="100dp"
         android:background="@drawable/button_ans"
         app:layout_constraintBottom_toBottomOf="@id/complete"
         app:layout_constraintStart_toEndOf="@id/reset"
         app:layout_constraintEnd_toEndOf="@id/complete"
         android:layout_marginBottom="70dp"
         />









</androidx.constraintlayout.widget.ConstraintLayout>